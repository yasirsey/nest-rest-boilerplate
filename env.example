# API
PORT=3000
NODE_ENV=development

# DATABASE
MONGODB_URI=mongodb://localhost:27017/your-database-name

# JWT
JWT_SECRET=your-jwt-secret-key
JWT_EXPIRES_IN=24h

# Rate Limiting
RATE_LIMIT_GENERAL_TTL=60000
RATE_LIMIT_GENERAL_LIMIT=100
RATE_LIMIT_AUTH_SUCCESS_TTL=60000
RATE_LIMIT_AUTH_SUCCESS_LIMIT=5
RATE_LIMIT_AUTH_FAILURE_TTL=900000
RATE_LIMIT_AUTH_FAILURE_LIMIT=3

# CORS
CORS_ORIGINS=http://localhost:5000,https://yourdomain.com
CORS_CREDENTIALS=false